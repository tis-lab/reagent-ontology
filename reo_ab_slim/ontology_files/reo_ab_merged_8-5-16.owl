    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000067">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/REO_0000248"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000052"/>
                <owl:someValuesFrom>
