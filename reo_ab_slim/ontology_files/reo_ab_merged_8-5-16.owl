    <!-- http://purl.obolibrary.org/obo/REO_0000000 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/REO_0000000">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
