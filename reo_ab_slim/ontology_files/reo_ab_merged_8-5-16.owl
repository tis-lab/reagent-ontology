    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/REO_0000906">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/RO_0002180"/>
        <rdfs:label>has_monomeric_unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/REO_0000907 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/REO_0000907">
